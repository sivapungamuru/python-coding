   "id": "76b8db97",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Even number: [2, 4, 6, 8]\n",
      "Odd number: [1, 3, 5, 7]\n"
     ]
    }
   ],
   "source": [
    "numbers=[1,2,3,4,5,6,7,8]\n",
    "even=[]\n",
    "odd=[]\n",
    "for x in numbers:\n",
    "    if(x%2==0):\n",
    "        even.append(x)\n",
    "    else:\n",
    "        odd.append(x)\n",
    "print(\"Even number:\",even)\n",
    "print(\"Odd number:\",odd)"
   ]
